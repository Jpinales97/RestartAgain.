<p id="alert"><%= alert %></p>
<h1>Log In</h1>
<%= form_tag sessions_path do |form| %>
    <div class="field">
        <%= label_tag :email %>
        <%= text_field_tag :email %>
    </div>
    <div class="field">
        <%= label_tag :password %>
        <%= password_field_tag :password %>
    </div>
    <div class="action">
        <%= submit_tag "Log In" %>
    </div>
<% end %>

<%= link_to 'Back', welcome_index_path %>
<%= link_to "Don't have an account?", signup_path %> 
